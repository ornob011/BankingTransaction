<!-- Header Section -->
<header class="header-section">
    <a href="/" class="site-logo">
        <h4 class="text-white">Easy Banking Transactions</h4>
    </a>
    <nav class="header-nav">
        <ul class="main-menu">
            <li><a href="/" class="active">Home</a></li>
            <li><a href="#about-us">About Us</a></li>
